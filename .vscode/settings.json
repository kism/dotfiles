{
  "files.associations": {
    ".stow-local-ignore": "plaintext",
    "**/.config/sway/*": "shellscript",
    ".vimrc": "viml"
  },
  "[plaintext]": {
    "files.trimTrailingWhitespace": false
  },

  "[ini]": {
    "files.trimTrailingWhitespace": false
  },
  "[viml]": {
    "files.trimTrailingWhitespace": false
  },
  "cSpell.words": [
    "asciiquarium",
    "cvsignore",
    "darcs",
    "detectdisplays",
    "Dotfiles",
    "gitmodules",
    "hyprland",
    "inputrc",
    "jordanbaird",
    "kierangee",
    "osascript",
    "shellcheckrc",
    "swaymsg",
    "windowtolayer"
  ],
  "shellcheck.ignorePatterns": {
    "**/*.csh": false,
    "**/*.cshrc": false,
    "**/*.fish": false,
    "**/*.login": false,
    "**/*.logout": false,
    "**/*.tcsh": false,
    "**/*.tcshrc": false,
    "**/*.xonshrc": false,
    "**/*.xsh": false,
    "**/*.zsh": false,
    "**/*.zshrc": false,
    "**/zshrc": false,
    "**/*.zprofile": false,
    "**/zprofile": false,
    "**/*.zlogin": false,
    "**/zlogin": false,
    "**/*.zlogout": false,
    "**/zlogout": false,
    "**/*.zshenv": false,
    "**/zshenv": false,
    "**/*.zsh-theme": false,
    "**/config": true,
    ".config/sway/*": true,
    "**/.config/sway/*": true
  }
}
