


<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>au.kierangee.detectdisplays</string>
        <key>Program</key>
        <string>/usr/bin/osascript</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/etc/detectdisplays.applescript</string>
        </array>
        <key>RunAtLoad</key>
        <true />
        <key>StandardErrorPath</key>
        <string>/tmp/au.kierangee.detectdisplays.stdout.log</string>
        <key>StandardOutPath</key>
        <string>/tmp/au.kierangee.detectdisplays.stderr.log</string>
        <key>StartInterval</key>
        <integer>60</integer>
    </dict>
</plist>
