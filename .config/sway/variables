set $term alacritty
set $browser vivaldi-stable --enable-features=UseOzonePlatform --ozone-platform=wayland
set $filemanager thunar
set $editor mousepad
set $launcher nwg-drawer -c 6 -is 64 -fscol 2 -term alacritty -pbexit 'nwg-dialog -p exit-sway -c "swaymsg exit"' -pblock 'nwg-lock' -pbpoweroff 'nwg-dialog -p poweroff -c "systemctl -i poweroff"' -pbreboot 'nwg-dialog -p reboot -c "systemctl reboot"' -pbsleep 'nwg-dialog -p sleep -c "systemctl suspend"' -pbsize 48
set $exit nwg-bar -p center -a middle -i 48
